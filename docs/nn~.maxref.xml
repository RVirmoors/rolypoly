<c74object name='nn~' category='audio,  deep learning,  ai'>

	<digest>Interface for deep learning models </digest>
	<description>Interface for deep learning models </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Antoine Caillon &amp; Axel Chemla--Romeu-Santos </metadata>
		<metadata name='tag'>audio</metadata>
		<metadata name='tag'>deep learning</metadata>
		<metadata name='tag'>ai</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

		<objarg name='model path' optional='1' type='symbol'>
			<digest>Absolute path to the pretrained model</digest>
			<description>Absolute path to the pretrained model. </description>
		</objarg>

		<objarg name='method' optional='1' type='symbol'>
			<digest>Name of the method to call during synthesis</digest>
			<description>Name of the method to call during synthesis. </description>
		</objarg>

		<objarg name='buffer size' optional='1' type='int'>
			<digest>Size of the internal buffer (can't be lower than the method's ratio)</digest>
			<description>Size of the internal buffer (can't be lower than the method's ratio). </description>
		</objarg>

	</objarglist>


	<!--MESSAGES-->

	<methodlist>

		<method name='anything'>
			<digest>callback for attributes </digest>
			<description>callback for attributes </description>
		</method>

	</methodlist>


	<!--ATTRIBUTES-->

	<attributelist>

		<attribute name='enable' get='1' set='1' type='long' size='1' >
			<digest>Enable / disable tensor computation </digest>
			<description>Enable / disable tensor computation </description>
		</attribute>

	</attributelist>


	<!--RELATED-->

	<seealsolist>
	</seealsolist>


</c74object>
